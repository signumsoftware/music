@using Signum.Test.Environment

@using (var nc = Html.TypeContext<NoteWithDateEntity>()) 
{
    @Html.ValueLine(nc, n => n.Mixin<CorruptMixin>().Corrupt)
	@Html.EntityLine(nc, n => n.Target, el => el.ReadOnly = true)
    @Html.ValueLine(nc, n => n.Text, vl => vl.ValueLineType = ValueLineType.TextArea)
	@Html.ValueLine(nc, n => n.CreationTime)
}
