@using Signum.Test.Environment

@using (var e = Html.TypeContext<BandEntity>()) 
{
	@Html.ValueLine(e, f => f.Name);
	@Html.EntityTabRepeater(e, f => f.Members, er => { er.Find = true; er.Move = true; });
    @Html.EntityLine(e, f => f.LastAward);
    @Html.EntityTabRepeater(e, f => f.OtherAwards);
}