<UserControl x:Class="Music.Windows.Controls.AmericanMusicAward"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:m="clr-namespace:Signum.Windows;assembly=Signum.Windows"
    xmlns:d="clr-namespace:Signum.Test.Environment;assembly=Signum.Test"
    m:Common.TypeContext="{x:Type d:AmericanMusicAwardEntity}"
    MinWidth="300">
    <StackPanel>
        <m:ValueLine m:Common.Route="Year" />
        <m:ValueLine m:Common.Route="Category" />
        <m:ValueLine m:Common.Route="Result" />
    </StackPanel>
</UserControl>
