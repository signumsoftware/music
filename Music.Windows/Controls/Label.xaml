<UserControl x:Class="Music.Windows.Controls.Label"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:m="clr-namespace:Signum.Windows;assembly=Signum.Windows"
    xmlns:d="clr-namespace:Signum.Test.Environment;assembly=Signum.Test"
    m:Common.TypeContext="{x:Type d:LabelEntity}"
    MinWidth="300">
    <StackPanel>
        <m:ValueLine m:Common.Route="Name" />
        <m:EntityLine m:Common.Route="Country" />
        <m:EntityLine m:Common.Route="Owner" />
    </StackPanel>
</UserControl>
